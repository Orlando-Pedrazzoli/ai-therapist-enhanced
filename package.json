{
  "name": "ai-therapist-enhanced",
  "version": "1.1.0",
  "description": "AI-powered mental wellness assistant with enhanced security and compliance",
  "author": "AI Wellness Team",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "setup": "node setup-env.js",
    "setup:keys": "node -e \"console.log('ENCRYPTION_MASTER_KEY=' + require('crypto').randomBytes(32).toString('base64')); console.log('AUDIT_ENCRYPTION_KEY=' + require('crypto').randomBytes(32).toString('base64')); console.log('NEXTAUTH_SECRET=' + require('crypto').randomBytes(32).toString('base64'));\"",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "node prisma/seed.js",
    "db:reset": "prisma migrate reset --force",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:env": "node scripts/test-env.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "clean": "rm -rf .next node_modules",
    "reinstall": "npm run clean && npm install",
    "analyze": "ANALYZE=true next build",
    "check:security": "npm audit",
    "check:updates": "npx npm-check-updates"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
